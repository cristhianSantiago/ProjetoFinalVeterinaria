<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="Sivet.master.cs" Inherits="Interface.Sivet" %>

<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title></title>
    <meta charset="utf-8" />
</head>
<body>
    <form id="form1" runat="server">
